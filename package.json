{
  "name": "can",
  "version": "4.1.1",
  "main": "can.js",
  "scripts": {
    "preversion": "npm test",
    "version": "git commit -am \"Update dist for release\" && git checkout -b release && git add -f dist/",
    "postversion": "git push --tags && git checkout - && git branch -D release && git push",
    "testee": "testee test/index.html --browsers firefox",
    "testee-production": "testee test/production.html --browsers firefox",
    "http-server": "http-server -p 3000 --silent",
    "test": "npm run test-development && npm run test-production",
    "test-development": "npm run build && npm run testee",
    "test-production": "npm run build && npm run build-tests && npm run testee-production",
    "ci": "npm run build && npm run test && node test/test-sauce-labs.js",
    "release:pre": "npm version prerelease && npm run build && npm publish --tag=pre",
    "release:patch": "npm version patch && npm run build && npm publish",
    "release:minor": "npm version minor && npm run build && npm publish",
    "release:major": "npm version major && npm run build && npm publish",
    "build": "node build.js",
    "build-tests": "node test/build.js",
    "document": "npm run deps-bundle && bit-docs",
    "document:force": "npm run deps-bundle && bit-docs -fd",
    "deps-bundle": "steal-tools bundle --deps --no-minify"
  },
  "title": "CanJS",
  "description": "MIT-licensed, client-side, JavaScript framework that makes building rich web applications easy.",
  "keywords": [
    "CanJS",
    "DoneJS"
  ],
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "web": "http://bitovi.com/"
  },
  "homepage": "http://canjs.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:canjs/canjs.git",
    "github": "https://github.com/canjs/canjs"
  },
  "dependencies": {
    "can-ajax": "1.1.4",
    "can-assign": "1.1.1",
    "can-attribute-encoder": "1.0.4",
    "can-component": "4.0.4",
    "can-compute": "4.0.3",
    "can-connect": "2.0.4",
    "can-connect-feathers": "4.0.0",
    "can-construct": "3.3.1",
    "can-construct-super": "3.1.4",
    "can-control": "4.0.2",
    "can-debug": "1.0.1",
    "can-define": "2.0.3",
    "can-define-backup": "1.0.0",
    "can-define-lazy-value": "1.0.2",
    "can-define-stream": "1.0.1",
    "can-define-stream-kefir": "1.0.0",
    "can-define-validate-validatejs": "1.0.1",
    "can-deparam": "1.0.3",
    "can-dom-data-state": "1.0.1",
    "can-dom-events": "1.1.3",
    "can-dom-mutate": "1.0.3",
    "can-event-dom-enter": "2.1.2",
    "can-event-dom-radiochange": "2.1.0",
    "can-event-queue": "1.0.1",
    "can-fixture": "2.0.3",
    "can-globals": "1.0.1",
    "can-kefir": "1.0.2",
    "can-key-tree": "1.0.2",
    "can-list": "4.0.0",
    "can-log": "1.0.0",
    "can-map": "4.0.1",
    "can-map-define": "4.0.0",
    "can-namespace": "1.0.0",
    "can-ndjson-stream": "0.1.8",
    "can-observation": "4.0.1",
    "can-observation-recorder": "1.0.2",
    "can-observe": "2.1.0",
    "can-param": "1.0.3",
    "can-parse-uri": "1.0.1",
    "can-queues": "1.0.1",
    "can-reflect": "1.13.3",
    "can-reflect-dependencies": "1.0.1",
    "can-reflect-promise": "2.0.1",
    "can-route": "4.1.1",
    "can-route-pushstate": "4.0.4",
    "can-set": "1.5.2",
    "can-simple-dom": "1.3.4",
    "can-simple-map": "4.0.1",
    "can-simple-observable": "2.0.2",
    "can-stache": "4.2.2",
    "can-stache-bindings": "4.1.0",
    "can-stache-converters": "4.0.2",
    "can-stache-key": "1.0.1",
    "can-stache-route-helpers": "0.1.4",
    "can-stream": "1.0.1",
    "can-stream-kefir": "1.0.1",
    "can-symbol": "1.6.1",
    "can-util": "3.11.5",
    "can-validate": "1.1.4",
    "can-validate-interface": "1.0.1",
    "can-validate-legacy": "1.2.0",
    "can-validate-validatejs": "0.1.3",
    "can-vdom": "4.0.1",
    "can-view-autorender": "4.0.0",
    "can-view-callbacks": "4.0.1",
    "can-view-live": "4.0.3",
    "can-view-model": "4.0.1",
    "can-view-nodelist": "4.0.2",
    "can-view-parser": "4.0.2",
    "can-view-scope": "4.0.2",
    "can-view-target": "4.0.1",
    "can-zone": "0.6.18",
    "react-view-model": "1.0.0"
  },
  "devDependencies": {
    "bit-docs": "0.0.8",
    "can-make-map": "^1.0.1",
    "can-reflect-tests": "<2.0.0",
    "can-test-helpers": "^1.1.1",
    "es6-promise": "^4.1.0",
    "feathers": "^2.0.3",
    "feathers-authentication-client": "^0.1.6",
    "feathers-hooks": "^1.7.1",
    "feathers-rest": "^1.5.0",
    "feathers-socketio": "^1.4.2",
    "funcunit": "^3.4.2",
    "github": "^11.0.0",
    "http-server": "^0.10.0",
    "jquery": "2.x - 3.x",
    "jquery-ui": "^1.12.0",
    "kefir": "^3.8.0",
    "prop-types": "^15.5.10",
    "qunitjs": "^2.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "semver": "^5.5.0",
    "socket.io-client": "^1.7.2",
    "steal": "^1.5.5",
    "steal-conditional": "^0.4.0",
    "steal-css": "^1.2.4",
    "steal-qunit": "^1.0.1",
    "steal-socket.io": "^4.0.9",
    "steal-stache": "^4.0.0",
    "steal-tools": "^1.3.6",
    "test-saucelabs": "0.0.3",
    "testee": "^0.7.0"
  },
  "demos": [
    "http://canjs.us/#examples",
    "http://canjs.us/recipes"
  ],
  "googleAnalyticsTrackingCode": "UA-2302003-11",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.php"
    }
  ],
  "steal": {
    "npmAlgorithm": "flat",
    "main": "can",
    "npmIgnore": [
      "bit-docs",
      "testee",
      "async",
      "saucelabs",
      "test-saucelabs",
      "wd",
      "http-server"
    ],
    "meta": {
      "socket.io-client/dist/socket.io": {
        "format": "cjs"
      }
    },
    "configDependencies": [
      "./node_modules/steal-conditional/conditional.js"
    ]
  },
  "bit-docs": {
    "html": {
      "dependencies": {
        "normalize.css": "^5.0.0",
        "steal-conditional": "^0.3.6",
        "steal-stache": "^4.0.1"
      },
      "package": {
        "steal": {
          "configDependencies": [
            "./node_modules/steal-conditional/conditional"
          ]
        }
      },
      "static": [],
      "templates": []
    },
    "dependencies": {
      "bit-docs-glob-finder": "^0.0.5",
      "bit-docs-dev": "^0.0.3",
      "bit-docs-js": "^0.0.6",
      "bit-docs-tag-sourceref": "^0.0.3",
      "bit-docs-generate-html": "^0.8.0",
      "bit-docs-generate-searchmap": "^0.2.0",
      "bit-docs-html-canjs": "^2.3.4",
      "bit-docs-prettify": "^0.3.0",
      "bit-docs-html-highlight-line": "^0.3.1",
      "bit-docs-tag-demo": "^0.5.1",
      "bit-docs-tag-package": "^0.0.5"
    },
    "glob": {
      "pattern": "{node_modules,docs}/{can-*,steal-stache,react-view-model}/**/*.{js,md}",
      "ignore": [
        "node_modules/can-namespace/**/*",
        "node_modules/can-wait/examples/**/*",
        "node_modules/can-*/dist/**/*",
        "node_modules/*/node_modules/**/*",
        "node_modules/can-debug/src/draw-graph/vis.js"
      ]
    },
    "altVersions": {
      "3.12.1": "https://v3.canjs.com",
      "2.3.33": "https://v2.canjs.com"
    },
    "parent": "canjs",
    "minifyBuild": true
  }
}
