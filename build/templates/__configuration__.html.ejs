<!DOCTYPE HTML>
<html>
<head>
	<link rel="stylesheet" type="text/css" href="<%= root %>/lib/qunit/qunit.css"/>
</head>
<body>
<h1 id="qunit-header"><%= configuration.name %> Test Suite</h1>

<h2 id="qunit-banner"></h2>

<div id="qunit-testrunner-toolbar"></div>
<h2 id="qunit-userAgent"></h2>
<ol id="qunit-tests"></ol>
<div id="qunit-test-area"></div>

<script type="text/javascript" src="<%= root %>/lib/steal.js"></script>
<script type="text/javascript" src="<%= root %>/lib/qunit/qunit.js"></script>
<script type="text/javascript">
	QUnit.config.autostart = false;

	steal(function() {
		window.CONFIGURATION = {
			steal: <%- JSON.stringify(configuration.steal) %>
		};

		steal.config(<%- JSON.stringify(configuration.steal) %>);
	}, "<%- modules.join('", "') %>")
	.then("<%- tests.join('", "') %>", function() {
		QUnit.start();
	});
</script>
</body>
</html>